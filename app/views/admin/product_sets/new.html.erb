<h1>New Product Set</h1>

<% form_for([:admin, @product_set], :html => {:multipart => true}) do |f| %>
  <%= f.error_messages %>

  <%= render :partial => f, :locals => { :f => f } %>

  <p>
    <%= f.submit "Create", :confirm => "Are you sure?" %>
  </p>
<% end %>

<%= link_to 'Back', admin_product_sets_path %>
