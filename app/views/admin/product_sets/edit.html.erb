<h1>Editing product_set</h1>

<% form_for([:admin, @product_set], :html => {:multipart => true}) do |f| %>
  <%= f.error_messages %>
  
  <%= render :partial => 'form', :locals => { :f => f } %>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', [:admin, @product_set] %> 
<%= link_to 'Back', admin_product_sets_path %>
